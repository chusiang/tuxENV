"1.2 (created: 2013/10/13 18:08:56)

set gui=nomenu,tabs,status,folderlist,mail

" Show Message Pane
nnoremap <F6> <F8>

" Quick switch gui style
nmap <F7> :set<Space>gui=all<Return>
nmap <F8> :set<Space>gui=nomenu,tabs,status,folderlist,mail<Return>
nmap <F9> :set<Space>gui=none<Return>


source! /home/jonny/.muttatorrc.local

