"1.4 (created: 2017/03/10 09:37:51)

" keymap

" - quick load muttator config.
map ,s :source! ~/.muttatorrc<Return>

" - Go to ...
map <C-g> :goto 

" - Archive.
map e <C-s>

" - Move mail to ...
map l :moveto 

" gui
map <F7> :set gui=all<Return>
map <F9> :set gui=mail,notabs,nostatus,nofolderlist<Return>

" - setting default layout.
set layout=wide

source! /Users/jonny/.muttatorrc.local

" vim: set ft=muttator:
